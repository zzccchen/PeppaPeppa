{
    "name": "PeppaPeppa",
    "version": "1.0",
    "description": "OCR验证码自动填写插件",
    "content_scripts": [{
        "matches": ["http://*/peppapeppa"],
        "js": ["jquery-3.3.1.min.js", "content.js"],
        "run_at": "document_start"
    }],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_title": "PeppaPeppa"
    },
    "permissions": ["tabs", "http://*/peppapeppa", "webRequest", "activeTab"],
    "manifest_version": 2
}